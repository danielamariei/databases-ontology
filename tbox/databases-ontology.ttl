#################################################################
#    General axioms
#################################################################

[ rdf:type owl:Restriction ;
  owl:onProperty :hasReplicationProtocol ;
  owl:someValuesFrom :ReplicationProtocol ;
  owl:equivalentClass [ rdf:type owl:Restriction ;
                        owl:onProperty :supportsReplication ;
                        owl:hasValue "true"^^xsd:boolean
                      ]
] .


[ rdf:type owl:Restriction ;
  owl:onProperty :hasDataModel ;
  owl:hasValue :GraphDataModel ;
  owl:equivalentClass [ rdf:type owl:Restriction ;
                        owl:onProperty :isSuitableForSoftwareApplication ;
                        owl:hasValue :RecommendationSoftwareApplication
                      ]
] .


[ rdf:type owl:Restriction ;
  owl:onProperty :hasStorageModel ;
  owl:hasValue :ColumnOriented ;
  owl:equivalentClass [ rdf:type owl:Restriction ;
                        owl:onProperty :isSuitableForSoftwareApplication ;
                        owl:hasValue :AnalyticsSoftwareApplication
                      ]
] .


[ rdf:type owl:Restriction ;
  owl:onProperty :hasStorageModel ;
  owl:hasValue :ColumnOriented ;
