:DocumentDBMS rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasDataModel ;
                                    owl:hasValue :DocumentDataModel
                                  ] ;
              rdfs:subClassOf :DatabaseManagementSystem ;
              rdfs:comment "The class of document DBMSs."@en ;
              rdfs:label "Document DBMS"@en .


###  http://www.semanticweb.org/ontologies/databases/Durability
:Durability rdf:type owl:Class ;
            rdfs:comment "The durability concept."@en ;
            rdfs:label "Durability"@en .


###  http://www.semanticweb.org/ontologies/databases/GraphDBMS
:GraphDBMS rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasDataModel ;
                                 owl:hasValue :GraphDataModel
                               ] ;
           rdfs:subClassOf :DatabaseManagementSystem ;
           rdfs:comment "The class of graph DBMSs."@en ;
           rdfs:label "Graph DBMS"@en .


###  http://www.semanticweb.org/ontologies/databases/InMemoryDBMS
:InMemoryDBMS rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasStorageTechnology ;
                                    owl:hasValue :InMemoryStorage
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :isSuitableForSoftwareApplication ;
                                    owl:hasValue :LowLatencySoftwareApplication
                                  ] ;
              rdfs:subClassOf :DatabaseManagementSystem ;
              rdfs:comment "The class of in-memory DBMSs."@en ;
              rdfs:label "In-Memory DBMS"@en .


###  http://www.semanticweb.org/ontologies/databases/KeyValueDBMS
:KeyValueDBMS rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasDataModel ;
                                    owl:hasValue :KeyValueDataModel
                                  ] ;
              rdfs:subClassOf :DatabaseManagementSystem ;
              rdfs:comment "The class of key-value DBMSs."@en ;
              rdfs:label "Key-Value DBMS"@en .


###  http://www.semanticweb.org/ontologies/databases/License
:License rdf:type owl:Class .


