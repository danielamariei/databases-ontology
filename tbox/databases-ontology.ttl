###  http://www.semanticweb.org/ontologies/databases/MultiModelDBMS
:MultiModelDBMS rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasDataModel ;
                                      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                      owl:onClass :DataModel
                                    ] ;
                rdfs:subClassOf :DatabaseManagementSystem ;
                rdfs:comment "The class of multi-model DBMSs."@en ;
                rdfs:label "Multi-Model DBMS"@en .


